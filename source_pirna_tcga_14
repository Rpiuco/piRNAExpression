## trocar os nomes dos genes para facilitar leitura
 #
 #

awk -F "\t" '{if($3=="gene"){split($9,arr,";");gsub(/gene_id /, "", arr[1]);gsub(/\"/, "", arr[1]);gsub(/ gene_type /, "", arr[2]);gsub(/\"/, "", arr[2]);gsub(/ gene_name /, "", arr[3]);gsub(/\"/, "", arr[3]);if(substr(arr[1],(length(arr[1])-4))!="PAR_Y"){print arr[1]"\t"arr[3]"\t"arr[2]}}}' /home/projects2/databases/gencode/release28/gencode.v28.annotation.gtf > rawData/ensemblToGenes.v28.txt

awk -F "\t" '{split($1,arr,".");printf "s/"arr[1]"/"$2"/\n"}' rawData/ensemblToGenes.v28.txt > rawData/codeToGene.ensembl.v28.sed

sed -f rawData/codeToGene.ensembl.v28.sed COAD/htSeqCount/matrix.genes.expression.control.txt



