awk -F "\t" '{if($13 =="T1"){print$1}}' metadata/metadata$tissueType/metadata.$tissueTypeLow\_panCancer_data.txt | for eachRow in `cut -f1 -`; do if [ -f "rawData/htSeqCountRaw/$eachRow.htseq.counts" ]; then join -1 1 -2 1 -t $'\t' $tissueType/htSeqCount/matrix.genes.expression_split.txt rawData/htSeqCountRaw/$eachRow.htseq.counts > $tissueType/htSeqCount/matrix.genes.expression_split.txt.temp; mv $tissueType/htSeqCount/matrix.genes.expression_split.txt.temp $tissueType/htSeqCount/matrix.genes.expression_split.txt; printf "\tT1_"$num >> $tissueType/htSeqCount/sampleHeader_ajcc.t1.txt;let num++; fi; done
