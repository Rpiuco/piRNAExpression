awk -F "\t" '{print "mv rawData/htSeqCountRaw/"$2".htseq.counts rawData/htSeqCountRaw/"$1".htseq.counts"}' rawData/caseId.COAD.tumor.txt > sourceTeste2
awk -F "\t" '{print "mv rawData/htSeqCountRaw/"$2"A.htseq.counts rawData/htSeqCountRaw/"$1".htseq.counts"}' rawData/caseId.COAD.normal.txt >> sourceTeste2
awk -F "\t" '{print "mv rawData/htSeqCountRaw/"$2".htseq.counts rawData/htSeqCountRaw/"$1".htseq.counts"}' rawData/caseId.BRCA.tumor.txt >> sourceTeste2
awk -F "\t" '{print "mv rawData/htSeqCountRaw/"$2"A.htseq.counts rawData/htSeqCountRaw/"$1".htseq.counts"}' rawData/caseId.BRCA.normal.txt >> sourceTeste2
awk -F "\t" '{print "mv rawData/htSeqCountRaw/"$2".htseq.counts rawData/htSeqCountRaw/"$1".htseq.counts"}' rawData/caseId.PRAD.tumor.txt >> sourceTeste2
awk -F "\t" '{print "mv rawData/htSeqCountRaw/"$2"A.htseq.counts rawData/htSeqCountRaw/"$1".htseq.counts"}' rawData/caseId.PRAD.normal.txt >> sourceTeste2
awk -F "\t" '{print "mv rawData/htSeqCountRaw/"$2".htseq.counts rawData/htSeqCountRaw/"$1".htseq.counts"}' rawData/caseId.PAAD.tumor.txt >> sourceTeste2
awk -F "\t" '{print "mv rawData/htSeqCountRaw/"$2"A.htseq.counts rawData/htSeqCountRaw/"$1".htseq.counts"}' rawData/caseId.PAAD.normal.txt >> sourceTeste2


