export tissueType="COAD";
export num=0;
cut -f 1 rawData/htSeqCountRaw/0000c4a9-f245-453e-956b-33aaa7edc88a.htseq.counts > $tissueType/htSeqCount/matrix.genes.expression.txt
for eachRow in `cut -f1 $tissueType/metadata_ajcc.control`; do if [ -f "rawData/htSeqCountRaw/$eachRow.htseq.counts" ]; then join -1 1 -2 1 -t $'\t' $tissueType/htSeqCount/matrix.genes.expression.txt rawData/htSeqCountRaw/$eachRow.htseq.counts > $tissueType/htSeqCount/matrix.genes.expression.txt.temp; mv $tissueType/htSeqCount/matrix.genes.expression.txt.temp $tissueType/htSeqCount/matrix.genes.expression.txt; printf "\tC_"$num >> $tissueType/htSeqCount/sampleHeader_ajcc.control.txt;let num++; fi; done
mv $tissueType/htSeqCount/matrix.genes.expression.txt $tissueType/htSeqCount/matrix.genes.expression.control.txt;

export num=0;
cut -f 1 rawData/htSeqCountRaw/0000c4a9-f245-453e-956b-33aaa7edc88a.htseq.counts > $tissueType/htSeqCount/matrix.genes.expression.txt
for eachRow in `cut -f1 $tissueType/metadata_ajcc.tumor`; do if [ -f "rawData/htSeqCountRaw/$eachRow.htseq.counts" ]; then join -1 1 -2 1 -t $'\t' $tissueType/htSeqCount/matrix.genes.expression.txt rawData/htSeqCountRaw/$eachRow.htseq.counts > $tissueType/htSeqCount/matrix.genes.expression.txt.temp; mv $tissueType/htSeqCount/matrix.genes.expression.txt.temp $tissueType/htSeqCount/matrix.genes.expression.txt; printf "\tT_"$num >> $tissueType/htSeqCount/sampleHeader_ajcc.tumor.txt;let num++; fi; done
mv $tissueType/htSeqCount/matrix.genes.expression.txt $tissueType/htSeqCount/matrix.genes.expression.tumor.txt;

