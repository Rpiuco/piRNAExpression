join -1 1 -2 1 -t $'\t' PAAD/expression_ajcc/matrix.pirna.expression.control PAAD/expression_pancan/matrix.pirna.expression.ductaltype | join -1 1 -2 1 -t $'\t' - PAAD/expression_pancan/matrix.pirna.expression.othertype | join -1 1 -2 1 -t $'\t' - PAAD/expression_pancan/matrix.pirna.expression.colloidtype | join -1 1 -2 1 -t $'\t' - PAAD/expression_pancan/matrix.pirna.expression.undifftype > PAAD/expression_pancan/matrix.pirna.sub.all
join -1 1 -2 1 -t $'\t' PAAD/expression_pancan/matrix.pirna.expression.ductaltype PAAD/expression_pancan/matrix.pirna.expression.othertype | join -1 1 -2 1 -t $'\t' - PAAD/expression_pancan/matrix.pirna.expression.colloidtype | join -1 1 -2 1 -t $'\t' - PAAD/expression_pancan/matrix.pirna.expression.undifftype > PAAD/expression_pancan/matrix.pirna.sub.all.withoutNorm



##
 #
 #
 #((NF-1)*0.1) -> percentagem minima de colunas que precisam ter valor maior que X. -1 quando a primeira coluna for um identificador
 #$i>0 -> se a coluna da linha possui valor maior que X ele adiciona um para a contagem
 #a[$1]>perc -> se a contagem for maior que a porcentagem minima printa a linha
 #for(i=X;i<=Y;i++ -> quando precisar pegar da coluna X(X=X+1) ate a Y (Y=Y+1)
 ## Nesse caso o NF tambem muda para a contagem de colunas usadas
 #
 # Usar cut -f1,X-Y para pegar as colunas quando for pegar os estagios diferentes
awk '{a[$1]=0;perc=((NF-1)*0.1)}{for(i=2;i<=NF;i++){if($i>0){a[$1]++}}}{if(a[$1]>perc){print $0}}' COAD/expression_ajcc/matrix.pirna.norxtum > COAD/expression_ajcc/matrix.pirna.norxtum.10p
