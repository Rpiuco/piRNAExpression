###Windows commandLine 
C:\xampp\mysql\bin\mysql.exe -u root -p -h localhost pirna_db -e "SELECT rel_pirna_target.pirna_access, target.target_code, target.target_name FROM rel_pirna_target INNER JOIN pirna ON rel_pirna_target.pirna_access = pirna.pirna_access AND pirna.pirna_organism = 'Homo sapiens' INNER JOIN target ON rel_pirna_target.target_code = target.target_code" > C:\linuxFiles\pirnaTargetFile.txt


for eachFile in `ls COAD/expression_ajcc/heatmap_005_10.*.all`; do IFS='.' read -a arr <<< "$eachFile"; cut -f 1 $eachFile > COAD/targets/targets.${arr[1]}.temp;awk -F " " 'NR==FNR{data[$2]=$1;next}{if(length(data[$3])>0){ data[$3]=substr(data[$3],0,15); print data[$3]"\t"$3"\t"$1 }}' rawData/ensemblToGenes.v28.txt rawData/pirnaTargetFile.txt | grep -w -f COAD/targets/targets.${arr[1]}.temp | sort -k1,1 | uniq -w 15 > COAD/targets/targets.${arr[1]}; rm -r COAD/targets/targets.${arr[1]}.temp; done;
for eachFile in `ls COAD/expression_pancan/heatmap_005_10.*.all`; do IFS='.' read -a arr <<< "$eachFile"; cut -f 1 $eachFile > COAD/targets/targets.${arr[1]}.temp;awk -F " " 'NR==FNR{data[$2]=$1;next}{if(length(data[$3])>0){ data[$3]=substr(data[$3],0,15); print data[$3]"\t"$3"\t"$1 }}' rawData/ensemblToGenes.v28.txt rawData/pirnaTargetFile.txt | grep -w -f COAD/targets/targets.${arr[1]}.temp | sort -k1,1 | uniq -w 15 > COAD/targets/targets.${arr[1]}; rm -r COAD/targets/targets.${arr[1]}.temp; done;

for eachFile in `ls BRCA/expression_ajcc/heatmap_005_10.*.all`; do IFS='.' read -a arr <<< "$eachFile"; cut -f 1 $eachFile > BRCA/targets/targets.${arr[1]}.temp;awk -F " " 'NR==FNR{data[$2]=$1;next}{if(length(data[$3])>0){ data[$3]=substr(data[$3],0,15); print data[$3]"\t"$3"\t"$1 }}' rawData/ensemblToGenes.v28.txt rawData/pirnaTargetFile.txt | grep -w -f BRCA/targets/targets.${arr[1]}.temp | sort -k1,1 | uniq -w 15 > BRCA/targets/targets.${arr[1]}; rm -r BRCA/targets/targets.${arr[1]}.temp; done;
for eachFile in `ls BRCA/expression_pancan/heatmap_005_10.*.all`; do IFS='.' read -a arr <<< "$eachFile"; cut -f 1 $eachFile > BRCA/targets/targets.${arr[1]}.temp;awk -F " " 'NR==FNR{data[$2]=$1;next}{if(length(data[$3])>0){ data[$3]=substr(data[$3],0,15); print data[$3]"\t"$3"\t"$1 }}' rawData/ensemblToGenes.v28.txt rawData/pirnaTargetFile.txt | grep -w -f BRCA/targets/targets.${arr[1]}.temp | sort -k1,1 | uniq -w 15 > BRCA/targets/targets.${arr[1]}; rm -r BRCA/targets/targets.${arr[1]}.temp; done;

