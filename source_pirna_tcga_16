###Windows commandLine 
C:\xampp\mysql\bin\mysql.exe -u root -p -h localhost pirna_db -e "SELECT rel_alignment_gene.gene_code, gene.gene_name, rel_pirna_alignment.pirna_access FROM rel_alignment_gene INNER JOIN rel_pirna_alignment ON rel_pirna_alignment.build_code = 'hg38' AND rel_pirna_alignment.alignment_id = rel_alignment_gene.alignment_id INNER JOIN gene ON rel_alignment_gene.gene_code = gene.gene_code" > C:\linuxFiles\pirnaOverlapFile.txt

for eachFile in `ls COAD/expression_ajcc/heatmap_005_10.*.all`; do IFS='.' read -a arr <<< "$eachFile"; cut -f 1 $eachFile > COAD/overlaps/overlaps.${arr[1]}.temp; grep -w -f COAD/overlaps/overlaps.${arr[1]}.temp rawData/pirnaOverlapFile.txt | sort -k1,1 | uniq -w 15 > COAD/overlaps/overlaps.${arr[1]}; rm -r COAD/overlaps/overlaps.${arr[1]}.temp; done;
